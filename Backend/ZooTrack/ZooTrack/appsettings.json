{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=Zootrack.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "ZooTrack.Services": "Debug"
    }
  },
  "AllowedHosts": "*",
  "CameraServiceSettings": {
    "TargetAnimals": [ "person", "man", "bottle", "sheep", "dog", "cow", "wolf", "tiger" ],
    "HighlightSavePath": "ZooTrack\\Media\\HighlightFrames"
  }
}
